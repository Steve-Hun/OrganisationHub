<%= form_with model: organisation do |form| %>
    
    <div>
        <%= form.label :name %>
        <%= form.text_field :name %>
    </div>
    <div>
        <%= form.label :country %>
        <%= form.text_field :country %>
    </div>
    <div>
        <%= form.label :language %>
        <%= form.text_field :language %>
    </div>

    <%= form.submit %>
<% end %>